{
  "name": "antarctic",
  "type": "module",
  "browserslist": [
    "last 6 versions",
    "not dead",
    "not IE 11"
  ],
  "scripts": {
    "add-build": "git add build/*",
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig & npm run stylelint & eslint source/",
    "build": "cross-env NODE_ENV=production gulp build && gulp imagemin",
    "start": "cross-env NODE_ENV=development gulp start",
    "webp": "gulp webp",
    "imagemin": "gulp imagemin"
  },
  "editorconfig-cli": [
    "*.{cjs,js,json,md,mjs}",
    "{gulp,source}/**/*.{html,js,json,md,mjs,scss,svg,twig}"
  ],
  "devDependencies": {
    "@babel/preset-env": "7.18.6",
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "browser-sync": "2.27.10",
    "circular-dependency-plugin": "5.2.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "del": "6.1.1",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "eslint": "7.32.0",
    "gulp": "4.0.2",
    "gulp-csso": "4.0.1",
    "gulp-data": "1.3.1",
    "gulp-eslint": "6.0.0",
    "gulp-group-css-media-queries": "1.2.2",
    "gulp-html-beautify": "1.0.1",
    "gulp-if": "3.0.0",
    "gulp-imagemin": "7.1.0",
    "gulp-lintspaces": "1.0.1",
    "gulp-postcss": "9.0.1",
    "gulp-posthtml": "3.0.5",
    "gulp-rename": "2.0.0",
    "gulp-sass": "5.1.0",
    "gulp-stacksvg": "1.0.6",
    "gulp-twig": "1.2.0",
    "gulp-webp": "4.0.1",
    "imagemin-mozjpeg": "10.0.0",
    "imagemin-pngquant": "9.0.2",
    "imagemin-svgo": "10.0.1",
    "postcss-reporter": "7.0.5",
    "postcss-scss": "4.0.5",
    "posthtml-bem-linter": "0.1.3",
    "posthtml-w3c-validator": "1.1.0",
    "pre-commit": "1.2.2",
    "sass": "1.53.0",
    "stylelint": "13.13.1",
    "typograf": "6.15.1",
    "webpack": "4.42.0",
    "webpack-stream": "5.2.1"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "pre-commit": [
    "build",
    "add-build"
  ]
}
